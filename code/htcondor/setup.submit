# The environment
universe              = vanilla
getenv                = True
request_cpus          = 1
request_memory        = 1G

# Execution
initialdir            = $ENV(HOME)/laminar_gradients
executable            = /usr/bin/bash

# Job
arguments   = $ENV(HOME)/laminar_gradients/code/setup.sh
log         = logs/setup_$(Cluster).$(Process).log
output      = logs/setup_$(Cluster).$(Process).out
error       = logs/setup_$(Cluster).$(Process).err
Queue