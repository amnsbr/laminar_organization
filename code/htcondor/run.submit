# The environment
universe              = vanilla
getenv                = True
request_cpus          = 1
request_memory        = 6G

# Execution
initialdir            = $ENV(HOME)/laminar_gradients
executable            = /usr/bin/bash

# Job
arguments   = $ENV(HOME)/laminar_gradients/code/run.sh
log         = logs/run_$(Cluster).$(Process).log
output      = logs/run_$(Cluster).$(Process).out
error       = logs/run_$(Cluster).$(Process).err
Queue
