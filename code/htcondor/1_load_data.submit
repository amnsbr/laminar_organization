# The environment
universe              = vanilla
getenv                = True
request_cpus          = 2
request_memory        = 8G

# Execution
initialdir            = $ENV(HOME)/laminar_gradients
executable            = /usr/bin/bash

# Job
arguments   = $ENV(HOME)/laminar_gradients/code/1_load_data.sh
log         = logs/1_load_data_$(Cluster).$(Process).log
output      = logs/1_load_data_$(Cluster).$(Process).out
error       = logs/1_load_data_$(Cluster).$(Process).err
Queue